#coaches
	h3
		background-image url('../img/trainers_h_bg.jpg')
		background-position center
		background-size cover
		background-repeat no-repeat
		line-height 202px
		font-size: 40px;
		font-family: "GeometriaBold";
		color: rgb(255, 255, 255);
		text-align: center;
		position relative
		&:before
			content url(../img/traners_ellips.png)
			position absolute
			top 0
			bottom 0
			left 0
			right 0
			width 1328px
			margin 0 auto
.slider_wrap
	position relative
	&:before
		content url(../img/traner_bg_2.jpg)
		position absolute
		top 0
		bottom 0
		left 0
		right 0
		width 1095px
		margin 0 auto
		z-index -1
	.container
		position relative
.photo
	float left
	margin-right 40px
.decription
	padding-right 60px
	h4
		font-size: 30px;
		font-family: "GeometriaBold";
		color: rgb(249, 131, 62);
		line-height: 2.133;
		color #313030
		padding-top 82px
		span
			color #f9833e
	h5
		font-size: 20px;
		font-family: "GeometriaMedium";
		color: rgb(38, 39, 39);
		line-height: 1.2;
		margin-bottom: 27px;
.coach_regs
	list-style none
	li
		font-size: 20px;
		font-family: "Geometria";
		color: rgb(38, 39, 39);
		line-height: 1.2;
		display table
		margin-bottom: 15px;
		&:before
			display table-cell
			content url('../img/disc.png')
			padding-right: 22px;
.owl-controlls .owl-buttons div
	border-radius: 50%;
	background-image: linear-gradient(-46deg, rgb(255,116,88) 0%, rgb(240,152,25) 100%);
	box-shadow: 0px 6px 8.55px 0.45px rgba(0, 0, 0, 0.19);
	width: 63px;
	height: 63px;
	z-index: 237;
	opacity 1
	line-height 51px
	font-size 34px
.owl-controlls
	position absolute
	left 0
	top 50%
	right 0
.owl-prev
	float left
.owl-next
	float right